{
  "name": "rpc-ws",
  "version": "0.6.3",
  "description": "JSON-RPC 2.0 implementation with websockets",
  "main": "dist/main.js",
  "module": "dist/main.mjs",
  "types": "dist/main.d.ts",
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "tsup"
  },
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js",
      "default": "./dist/main.js"
    },
    "./frontend": {
      "import": "./dist/frontend.mjs",
      "require": "./dist/frontend.js",
      "default": "./dist/frontend.js"
    }
  },
  "keywords": [
    "websocket",
    "json",
    "rpc"
  ],
  "author": "Carlos Souza <carlos.pessoal@hotmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlos8v/rpc-ws.git"
  },
  "dependencies": {
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "@types/ws": "^8.5.4",
    "rimraf": "^4.3.1",
    "tslib": "^2.5.0",
    "tsup": "^6.6.3",
    "tsx": "^3.12.5",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2"
  }
}
